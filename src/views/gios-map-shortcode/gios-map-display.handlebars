<div id="gios-map-container">
  <div class="title-bar row">
    <div id="country-name" class="col-sm-8"></div>
    <div class="col-sm-4 controls">
      <i id="map-zoom-in" class="fa fa-plus-circle map-control" >&nbsp;</i>
      <i id="map-zoom-out" class="fa fa-minus-circle map-control" >&nbsp;</i>
      <i id="details-close" class="fa fa-times-circle map-control map-control-disabled">&nbsp;</i>
    </div>
  </div>
  <div class="map-container">
    <div class="overlay"></div>
    <div id="research-map" class="gios-research-map"></div>
  </div>
  <div id="details" class="container"></div>
  {{#if disclaimer}}
    <div class="disclaimer small text-center">
      This map represents only a snapshot of selected sustainability research at ASU. Information may not be complete and is subject to change. <a href="/research">More details on our research</a>.
    </div>
  {{/if}}
  <div class="tool-tip"></div>
</div>
